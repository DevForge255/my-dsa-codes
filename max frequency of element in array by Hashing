#include <iostream>
using namespace std;
int main(){
    int arr[]={1,2,2,4,4,12,12,4,4,6,7,3,3,5,5,5,7};
    int n ;
    int n1=sizeof(arr)/4;
    
    
    //pre compute 
    int hash[13]={0};
    for(int i=0;i<n1;i++ ){
        
        hash[arr[i]]++;
    }
    int a;
    int mx=0;
     for(int j=0;j<13;j++ ){
         
         if (mx<hash[j]){
             
             mx=hash[j];
             a=j;
         }
         
     }
     cout<<"max frequency is of number "<<a<<" the frequency is "<<mx;
}
